{{#each _items as |item|}}
  {{component _type
    value=item.value
    title=item.title
    disabled=item.disabled
    activeMood=item.activeMood
    inactiveMood=item.inactiveMood
    icon=item.icon
    activeIcon=item.activeIcon
    inactiveIcon=item.inactiveIcon
    size=size
    group=this
  }}
{{/each}}
{{yield this}}
